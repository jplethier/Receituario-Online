div#pagina { margin: 0 auto; width: 1100px; }

div#cabecalho { background: #003399; position: static; float: left; width: 180px; margin: 10px 0; border: black solid; border-radius: 10px; }

div#cabecalho div.menuCabecalho { padding: 0; width: 150px; }

div#cabecalho div.menuCabecalho ul {}

div#cabecalho div.menuCabecalho ul li {}

div#cabecalho div.menuCabecalho ul li a { text-decoration: none; color: #66FF66; }

div#cabecalho div.menuCabecalho ul li a:hover { text-decoration: underline; color: #66FF66; }

div#cabecalho div.menuUsuario { padding: 0; width: 150px; }

div#cabecalho div.menuUsuario ul {}

div#cabecalho div.menuUsuario ul li {}

div#cabecalho div.menuUsuario ul li a { text-decoration: none; color: #66FF66; }

div#cabecalho div.menuUsuario ul li a:hover { text-decoration: underline; color: #66FF66; }

div#corpo { background: #66CCCC; float: right; width: 880px; margin: 10px 20px 10px 5px; border-radius: 10px; border-color: #6699CC; }

body { background: #6699CC; margin: 0; padding: 0; display: block; }

div#informacoes_usuario { margin: 5px 0 10px; background: #003399; border: black solid; border-radius: 10px; padding: 2px 5px; }

div#informacoes_usuario span { color: white; }

div#informacoes_usuario span strong { color: black; }

div.msg_erro { background: none repeat scroll 0 0 Salmon; border: medium solid Red; border-radius: 10px; margin: 5px; padding: 2px; }
div.msg_aviso { background: none repeat scroll 0 0 Moccasin; border: medium solid Yellow; border-radius: 10px; margin: 5px; padding: 2px; }
div.msg_sucesso { background: none repeat scroll 0 0 lightGreen; border: medium solid DarkGreen; border-radius: 10px; margin: 5px; padding: 2px; }