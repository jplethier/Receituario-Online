div#pagina {
    margin: 0 auto;
    width: 100%;
}
div#cabecalho {
    background-color: whiteSmoke;
    border: medium solid black;
    border-radius: 3px 3px 3px 3px;
    float: left;
    margin: 10px 0 0 100px;
    position: static;
    width: 15%;
}
div#cabecalho div.menuCabecalho {
    padding: 0;
    text-align: left;
    width: 70%;
}
div#cabecalho div.menuCabecalho ul {
    list-style: none outside none;
    width: 100%;
}
div#cabecalho div.menuCabecalho ul li:hover {
    background-color: window;
}
div#cabecalho div.menuCabecalho ul li {
    border-bottom: 1px solid;
    padding-bottom: 8px;
    padding-left: 0;
    padding-top: 8px;
}
div#cabecalho div.menuCabecalho ul li a {
    color: #000000;
    text-decoration: none;
}
div#cabecalho div.menuCabecalho ul li a:hover {
    color: black;
    text-decoration: none;
}
div#cabecalho div.menuUsuario {
    padding: 0;
    text-align: left;
    width: 70%;
}
div#cabecalho div.menuUsuario ul {
    list-style: none outside none;
    width: 100%;
}
div#cabecalho div.menuUsuario ul li:hover {
    background-color: window;
}
div#cabecalho div.menuUsuario ul li {
    border-bottom: 1px solid;
    padding-bottom: 8px;
    padding-left: 0;
    padding-top: 8px;
}
div#cabecalho div.menuUsuario ul li a {
    color: #000000;
    text-decoration: none;
}
div#cabecalho div.menuUsuario ul li a:hover {
    color: black;
    text-decoration: none;
}
div#corpo {
    background: none repeat scroll 0 0 WhiteSmoke;
    border: 3px solid;
    border-radius: 3px 3px 3px 3px;
    float: left;
    margin: 10px 20px 10px 15px;
    min-height: 480px;
    padding: 10px;
    width: 60%;
}
body {
    background: url("http://www.sitemedico.com.br/site/templates/ja_teline_iii/images/pattern_0.gif") repeat scroll 0 0 transparent;
    display: block;
    margin: 0;
    padding: 0;
}
div#informacoes_usuario {
    background: none repeat scroll 0 0 #003399;
    border: medium solid black;
    border-radius: 10px 10px 10px 10px;
    margin: 5px 0 10px;
    padding: 2px 5px;
}
div#informacoes_usuario span {
    color: white;
}
div#informacoes_usuario span strong {
    color: black;
}
div.msg_erro {
    background: none repeat scroll 0 0 Salmon;
    border: medium solid Red;
    border-radius: 3px 3px 3px 3px;
    margin: 5px;
    padding: 2px;
}
div.msg_aviso {
    background: none repeat scroll 0 0 Moccasin;
    border: medium solid Yellow;
    border-radius: 3px 3px 3px 3px;
    margin: 5px;
    padding: 2px;
}
div.msg_sucesso {
    background: none repeat scroll 0 0 lightGreen;
    border: medium solid DarkGreen;
    border-radius: 3px 3px 3px 3px;
    margin: 5px;
    padding: 2px;
}
div#header {
    background-color: PowderBlue;
    border: 1px solid;
    float: left;
    height: 150px;
    width: 100%;
}
div#logo {
    background: url("http://www.phenforum.com/images/phentermine/prescription.png") no-repeat scroll 0 0 transparent;
    float: left;
    height: 125px;
    margin-left: 100px;
    margin-top: 20px;
    width: 15%;
}
div#slogan {
    float: left;
    height: 88px;
    margin-left: 22px;
    margin-top: 20px;
    padding: 20px;
    text-align: left;
    width: 65%;
}

